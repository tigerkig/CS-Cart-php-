msgctxt "Addons::name::onekpay"
msgid "BillriantPay payments"
msgstr "תשלומים BillriantPay"

msgctxt "Addons::description::onekpay"
msgid "Lets you accept payments via BillriantPay."
msgstr "מאפשר לך לקבל תשלומים באמצעות BillriantPay."

msgctxt "Languages::onekpay.merchant_number"
msgid "Merchant ID"
msgstr "סוחר ID"

msgctxt "Languages::onekpay.terminal_number"
msgid "Terminal ID"
msgstr "מסוף ID"

