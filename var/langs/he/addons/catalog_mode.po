msgctxt "Addons::name::catalog_mode"
msgid "Catalog mode"
msgstr "מצב קטלוג"

msgctxt "Addons::description::catalog_mode"
msgid "Allows you to use the store as a product catalog"
msgstr "מאפשר לך להשתמש בחנות כקטלוג מוצרים"

msgctxt "Languages::catalog_mode"
msgid "Catalog Mode"
msgstr "מצב קטלוג"

msgctxt "Languages::text_catalog_mode_zero_price_action_notice"
msgid "In order that the \"Buy now\" button could be displayed on the product details page, please set \"Zero price action\" as \"Allow customers to add the product to cart\"."
msgstr "על מנת שהכפתור \"קנה עכשיו\" יכול להיות מוצג בדף פרטי המוצר, הגדר \"אפס מחיר פעולה\" כמו \"אפשר ללקוחות להוסיף את המוצר לסל\"."

msgctxt "Languages::buy_now_url"
msgid "Buy Now URL"
msgstr "לקנות עכשיו URL"

msgctxt "SettingsSections::catalog_mode::general"
msgid "General"
msgstr "כללי"

msgctxt "SettingsOptions::catalog_mode::main_store_mode"
