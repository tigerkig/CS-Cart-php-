msgid "Type"
msgstr "タイプ"

msgctxt "Languages::warehouses.store_type_store"
msgid "Store"
msgstr "店舗"

msgctxt "Languages::warehouses.store_type_pickup"
msgid "Pickup point"
msgstr "受取場所"

